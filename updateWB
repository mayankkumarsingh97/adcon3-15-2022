      cal_point = $(this).find('.nomVal').val()
      cal_point_unit = $(this).closest('table').find('.nomValUnit option:selected').val()

      // $(this).find('.wo_uuc option:selected').val();

      obs_point_unit = $(this).closest('table').find(".obsValUnit option:selected").val()

