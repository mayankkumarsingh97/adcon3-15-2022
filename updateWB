<script>
   $(".instruselect").change(function () {

     arrayInstru = []

     $('.instruselect').each(function() {
     var instrument = $(this).find(' option:selected').val();
     var dictInstru = {};
     dictInstru['instrument'] =instrument
     if(dictInstru["instrument"] != ''){
      arrayInstru.push(dictInstru);

      $.ajax({
        type: 'POST',
        url: '{% url 'observation:getContact' %}',
        data: {
          'arrayIN[]': JSON.stringify(arrayInstru),
          csrfmiddlewaretoken:$('input[name=csrfmiddlewaretoken]').val(),
          action: 'post',
        },
        success: function (data) {
          console.log(data)
          $(".coolAddress").html(data);
        }

      });

      }
      });

   });
</script>
<script>
   $(document).ready(function() {
     $( ".obsN " ).change(function() {

       val1 = $(this).closest("tr").find(".obs1").val();
       val2 = $(this).closest("tr").find(".obs2").val();
       val3 = $(this).closest("tr").find(".obs3").val();
       val4 = $(this).closest("tr").find(".obs4").val();
       starting_temp = $(this).closest("tr").find(".starting_temp").val();
       ending_temp = $(this).closest("tr").find(".ending_temp").val();
       middle_temp = $(this).closest("tr").find(".middle_temp").val();


       ea = $(".Ea").val();
       eb = $(".Eb").val();
       ec = $(".Ec").val();
       ed = $(".Ed").val();
       eo = $(".Eo").val();


       avgtemp = (parseFloat(starting_temp) + parseFloat(ending_temp) + parseFloat(middle_temp))/3

       averageVale = (parseFloat(val1) + parseFloat(val2) + parseFloat(val3) + parseFloat(val4) )/4


      abc = $(this).closest("tr").parent().closest('tr').find(".totalE").val(averageVale);
      maxx= Math.max(ea,eb,ec,ed,eo)

      initVal = parseFloat($(this).closest("tr").parent().closest('tr').find(".nomVal").val());

      calcVal = (initVal-averageVale)

      $(this).closest("tr").parent().closest('tr').find(".errorE").val(calcVal);
      $('#maxValE').val(maxx);


   });


   });
</script>
<script>
   $(document).ready(function() {
     $( ".En " ).change(function() {


       ea = $(".Ea").val();
       eb = $(".Eb").val();
       ec = $(".Ec").val();
       ed = $(".Ed").val();
       eo = $(".Eo").val();



      maxx= Math.max(ea,eb,ec,ed,eo)
      minn= Math.min(ea,eb,ec,ed,eo)
      diff = maxx-minn

      $('#maxValE').val(maxx);
      $('#minValE').val(minn);
      $('#diFF').val(diff);


   });


   });
</script>
<script>
   $(document).ready(function() {
     $( ".Repeatability" ).change(function() {

       HR1 = $(".HR1").val();
       HR2 = $(".HR2").val();
       HR3 = $(".HR3").val();
       HR4 = $(".HR4").val();
       HR5 = $(".HR5").val();
       HR6 = $(".HR6").val();
       HR7 = $(".HR7").val();
       HR8 = $(".HR8").val();
       HR9 = $(".HR9").val();
       HR10 = $(".HR10").val();

       FR1 = $(".FR1").val();
       FR2 = $(".FR2").val();
       FR3 = $(".FR3").val();
       FR4 = $(".FR4").val();
       FR5 = $(".FR5").val();
       FR6 = $(".FR6").val();
       FR7 = $(".FR7").val();
       FR8 = $(".FR8").val();
       FR9 = $(".FR9").val();
       FR10 = $(".FR10").val();


       if (HR6 != '') {
       sumNH = (parseFloat(HR1)+parseFloat(HR2)+parseFloat(HR3)+parseFloat(HR4)+parseFloat(HR5)+parseFloat(HR6)+parseFloat(HR7)+parseFloat(HR8)+parseFloat(HR9)+parseFloat(HR10))
       avgNH = sumNH/10

       sumNF = (parseFloat(FR1)+parseFloat(FR2)+parseFloat(FR3)+parseFloat(FR4)+parseFloat(FR5)+parseFloat(FR6)+parseFloat(FR7)+parseFloat(FR8)+parseFloat(FR9)+parseFloat(FR10))
       avgNF = sumNF/10

       } else {
       sumNH = (parseFloat(HR1)+parseFloat(HR2)+parseFloat(HR3)+parseFloat(HR4)+parseFloat(HR5))
       avgNH = sumNH/5

       sumNF = (parseFloat(FR1)+parseFloat(FR2)+parseFloat(FR3)+parseFloat(FR4)+parseFloat(FR5))
       avgNF = sumNF/5

       }


      $('.AvgHL').val(avgNH);
      $('.AvgFL').val(avgNF);



   });


   });
</script>
<script>
   $(document).delegate('.fetchButton', 'click', function(e) {
    $("tr.apme").each(function() {
      if($(this).find('.obs1').val()!=''){

      duc_id='{{ srfN.instrument.id }}';
      lc_duc = $(this).closest('.itemTYCard').find('.lc_duc').val()
      lc_duc_unit = $(this).closest('.itemTYCard').find('.lc_duc_unit').val()


