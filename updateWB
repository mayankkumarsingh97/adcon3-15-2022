      obs2 = $(this).find('.obs2').val()

      obs3 = $(this).find('.obs3').val()

      obs4 = $(this).find('.obs4').val()

      obs5 = $(this).find('.obs5').val()

      nomUnit = $(this).find('.nomUnit option:selected').val()
      obsUnit = $(this).find('.obsUnit option:selected').val()


