      FR2 = $(".FR2").val()
      FR3 = $(".FR3").val()
      FR4 = $(".FR4").val()
      FR5 = $(".FR5").val()
      FR6 = $(".FR6").val()
      FR7 = $(".FR7").val()
      FR8 = $(".FR8").val()
      FR9 = $(".FR9").val()
      FR10 = $(".FR10").val()
      range= $('.range').val()

      middle_temp= $(this).closest('.itemTYCard').find('.middle_temp').val()
      least_count= $(this).closest('.itemTYCard').find('.least_count').val()

      avgtemp = (parseFloat(starting_temp) + parseFloat(ending_temp) + parseFloat(middle_temp))/3
      // avgtemp= $(this).find('.avgtemp').val()

      tempP = avgtemp

               $.ajax({
                     type:'POST',
                     url:'{% url 'observation:weighingUncertainView' %}',
                     context: this,
                     data:{
                          duc_id: duc_id,
                          lc_duc: lc_duc,
                          lc_duc_unit:lc_duc_unit,
                          lc_duc_unit_parameter: lc_duc_unit_parameter,
                          cal_point: cal_point,
                          cal_point_unit: cal_point_unit,
                          obs_point_unit: obs_point_unit,
                          obs1: obs1,
                          obs2: obs2,
                          obs3: obs3,
                          obs4: obs4,
                          obs5: obs5,
                          nomUnit :nomUnit,
                          obsUnit :obsUnit,
                          HR1: HR1,
                          HR2: HR2,
                          HR3: HR3,
                          HR4: HR4,
                          HR5: HR5,
                          HR6: HR6,
                          HR7: HR7,
                          HR8: HR8,
                          HR9: HR9,
                          HR10: HR10,
                          FR1: FR1,
                          FR2: FR2,
                          FR3: FR3,
                          FR4: FR4,
                          FR5: FR5,
                          FR6: FR6,
                          FR7: FR7,
                          FR8: FR8,
                          FR9: FR9,
                          FR10: FR10,
                          range:range,
                          starting_temp:starting_temp,
                          ending_temp:ending_temp,
                          middle_temp:middle_temp,
                          avgtemp:avgtemp,
                          tempP:tempP,
                          least_count:least_count,
                          eccentricity_max_min:eccentricity_max_min,

                         csrfmiddlewaretoken:$('input[name=csrfmiddlewaretoken]').val(),
                         action: 'post'
                     },
                     data_type : 'html',
                     success: function (data) {
                       console.log(data)



                       unc = $(this).find('.uncertainP').val(JSON.parse(data)["Up"])
                       unc2 = $(this).find('.leastE').val(JSON.parse(data)["U"])

                     },
                     error: function (data) {
                         console.log("Got an error dude " + data);
                     }
                 });

    }

    });
    });



</script>
<script>
   jQuery(document).delegate('a.add-record', 'click', function(e) {
     // e.preventDefault();

     $('#tbl_posts').append($('#step_table tr:last td:last').html());

       var content = jQuery('#sample_table > tbody > tr').not("#tbl_posts tbody tr table ");

       size = jQuery('#tbl_posts >tbody >tr').length -2,
       element = null,
       element = content.clone(true);
       element.attr('id', 'rec-'+size);
       element.attr('class', 'item apme');

       element.find('.delete-record').attr('data-id', size);
       element.appendTo('#tbl_posts_body');
       element.find('.sn').html(size);




       });
