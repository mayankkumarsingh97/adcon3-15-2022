<script>
  $(document).ready(function() {
      $( ".vee" ).change(function() {

          Eccentricity1 = $(this).find(".E1").val();
          console.log(Eccentricity1)
          Eccentricity_INT_1 = parseFloat(Eccentricity1)
          Eccentricity2 = $(this).find(".E2").val();
          Eccentricity_INT_2 = parseFloat(Eccentricity2)
          Eccentricity3 = $(this).find(".E3").val();
          Eccentricity_INT_3 = parseFloat(Eccentricity3)
          console.log(Eccentricity3)
          Eccentricity4 = $(this).find(".E4").val();
          Eccentricity_INT_4 = parseFloat(Eccentricity4)
          Eccentricity5 = $(this).find(".E5").val();
          Eccentricity_INT_5 = parseFloat(Eccentricity5)
          Eccentricity6 = $(this).find(".E6").val();
          Eccentricity_INT_6 = parseFloat(Eccentricity6)
          Eccentricity7 = $(this).find(".E7").val();
          Eccentricity_INT_7 = parseFloat(Eccentricity7)
          Eccentricity8 = $(this).find(".E8").val();
          Eccentricity_INT_8 = parseFloat(Eccentricity8)
          Eccentricity9 = $(this).find(".E9").val();
          Eccentricity_INT_9 = parseFloat(Eccentricity9)
          Eccentricity10 = $(this).find(".E10").val();
          Eccentricity_INT_10 = parseFloat(Eccentricity10)
          Eccentricity11 = $(this).find(".E11").val();
          Eccentricity_INT_11 = parseFloat(Eccentricity11)



          Eccentricity_1_6_11 = (Eccentricity_INT_1 + Eccentricity_INT_6 + Eccentricity_INT_11)/3
          $('.obs21').val(Eccentricity_1_6_11)

          Eccentricity_2_10 = (Eccentricity_INT_2 + Eccentricity_INT_10)/2
          $('.obs22').val(Eccentricity_2_10)

          Eccentricity_3_9 = (Eccentricity_INT_3 + Eccentricity_INT_9)/2
          $('.obs23').val(Eccentricity_3_9)

          Eccentricity_4_8 = (Eccentricity_INT_4 + Eccentricity_INT_8)/2
          $('.obs24').val(Eccentricity_4_8)

          Eccentricity_5_7 = (Eccentricity_INT_5 + Eccentricity_INT_7)/2
          $('.obs25').val(Eccentricity_5_7)

          diff_obs1_INT_01 = Eccentricity_1_6_11 - Eccentricity_2_10
          diff_obs1_INT_02 = Eccentricity_1_6_11 - Eccentricity_3_9
          diff_obs1_INT_03 = Eccentricity_1_6_11 - Eccentricity_4_8
          diff_obs1_INT_04 = Eccentricity_1_6_11 - Eccentricity_5_7

          $('.obsdiff21').val(diff_obs1_INT_01)
          $('.obsdiff22').val(diff_obs1_INT_02)
          $('.obsdiff23').val(diff_obs1_INT_03)
          $('.obsdiff24').val(diff_obs1_INT_04)




      })
      });

</script>

