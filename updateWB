   $(document).ready(function() {
     $( ".nomUnit " ).change(function() {
     unit_nom = $('.nomUnit').val()
     abc = $("#nomS").val(unit_nom)
     });

     $( ".obsUnit" ).change(function() {
     unit_obs = $('.obsUnit').val()
     abc = $(".obsS").val(unit_obs)
     });

     unit_nom = $('.nomUnit').val()
     abc = $("#nomS").val(unit_nom)
     unit_obs = $('.obsUnit').val()
     abcd = $(".obsS").val(unit_obs)


     });
